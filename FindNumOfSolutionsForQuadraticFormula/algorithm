public class FindNumOfSolutionsForQuadraticFormula {


	public static void main(String arg[]){
	System.out.println(solutions(1,0,-1));

	}
	public static int solutions(int a, int b, int c) {
		int x=b*b-4*a*c;
		if(x<0){
			return 0;
		}
		else if(x==0){
			return 1;
		}
		else{
			return 2;
		}
  	}
}
